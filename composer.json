{
  "name": "schleyer-edv/fireapi-php-client",
  "description": "A full-featured implementation of fireapi",
  "license": "MIT",
  "type": "library",
  "keywords": ["reselling", "24fire", "fireapi", "RESTful-API", "KVM", "Dedicated"],
  "authors": [
    {
      "name": "Schleyer-EDV, BjÃ¶rn Schleyer",
      "email": "b.schleyer@schleyer-edv.de",
      "homepage": "https://www.schleyer-edv.de/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@schleyer-edv.de",
    "issues": "https://github.com/schleyer-edv/fireapi-php-client/issues",
    "source": "https://github.com/schleyer-edv/fireapi-php-client"
  },
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.8|~7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "fireapi\\": "src/"
    }
  },
  "autoload-dev": {
    "Tests\\": "tests/"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}